<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!--the shadow comes from here-->
    <item
        android:bottom="0dp"
        android:left="0dp"
        android:right="0dp"
        android:top="0dp">
        <shape android:shape="rectangle">
            <corners
                android:radius="10dp"
                />
            <solid android:color="#FFFFFF" />
            <stroke
                android:color="#19267E"
                android:width="1dp" />
        </shape>
    </item>
</layer-list>